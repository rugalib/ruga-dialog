{
  "name": "rugalib/ruga-dialog",
  "description": "jquery ui dialog library for ruga framework",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "laminas/laminas-json": "^3.3",
    "rugalib/ruga-asset": "^1.0",
    "rugalib/asset-jquery": "^3.6",
    "rugalib/asset-jqueryui": "^1.13"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "laminas/laminas-config-aggregator": "^1",
    "laminas/laminas-servicemanager": "^3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.rugalib.ch/"
    }
  ],
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Dialog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruga\\Dialog\\Test\\": "test/src"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Dialog",
      "config-provider": "Ruga\\Dialog\\ConfigProvider"
    }
  }
}